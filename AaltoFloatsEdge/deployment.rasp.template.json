{
  "$schema-template": "4.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "https": {
                "username": "$CONTAINER_REGISTRY_USERNAME_https",
                "password": "$CONTAINER_REGISTRY_PASSWORD_https",
                "address": "$CONTAINER_REGISTRY_address_https"
              },
              "aaltofloats": {
                "username": "$CONTAINER_REGISTRY_USERNAME_aaltofloats",
                "password": "$CONTAINER_REGISTRY_PASSWORD_aaltofloats",
                "address": "aaltofloats.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "dvlHandler": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.dvlHandler}",
              "createOptions": {}
            }
          },
          "SimulatedDVL": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.SimulatedDVL}",
              "createOptions": {}
            }
          },
          "dvlmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.dvlmodule}",
              "createOptions": {}
            }
          },
          "GNSSModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.GNSSModule}",
              "createOptions": {}
            }
          },
          "ctdModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.ctdModule}",
              "createOptions": {}
            }
          },
          "localizationModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.localizationModule}",
              "createOptions": {}
            }
          },
          "audioModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.audioModule}",
              "createOptions": {}
            }
          },
          "diveControlModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.diveControlModule}",
              "createOptions": {}
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.2",
        "routes": {
          "GNSSModuleToIoTHub": "FROM /messages/modules/GNSSModule/outputs/* INTO $upstream",
          "localizationModuleToIoTHub": "FROM /messages/modules/localizationModule/outputs/* INTO $upstream",
          "audioModuleToIoTHub": "FROM /messages/modules/audioModule/outputs/* INTO $upstream",
          "diveControlModuleToIoTHub": "FROM /messages/modules/diveControlModule/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}